    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "canvas": "^2.11.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
